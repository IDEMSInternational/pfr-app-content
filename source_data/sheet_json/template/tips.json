{
  "flow_type": "template",
  "flow_name": "tips",
  "status": "released",
  "rows": [
    {
      "name": "data_list_name",
      "value": "tips"
    },
    {
      "name": "id",
      "value": "rel_vacuum"
    },
    {
      "name": "join_image",
      "value": "_image_asset"
    },
    {
      "name": "join_text",
      "value": "_text"
    },
    {
      "name": "tips_data_row",
      "value": "@local.data_list_name.@local.id"
    },
    {
      "name": "tips_data",
      "value": "@data.@local.tips_data_row"
    },
    {
      "name": "no_of_parts_list",
      "value": "!!@local.row_data.text; "
    },
    {
      "name": "no_of_parts",
      "value": "@calc(@local.no_of_parts_list.filter(Boolean).length)",
      "condition": "!@fields.currently_on_last"
    },
    {
      "name": "no_of_parts",
      "value": "@calc(@local.no_of_parts_list.filter(Boolean).length+1)",
      "condition": "@fields.currently_on_last"
    },
    {
      "name": "show_part_no",
      "value": 1
    },
    {
      "type": "title",
      "name": "title",
      "value": "@local.tips_data.title",
      "condition": "!!(@local.tips_data.title)"
    },
    {
      "type": "begin_display_group",
      "name": "dg_images"
    },
    {
      "type": "image",
      "name": "image_1_asset",
      "value": "@local.tips_data.image_1_asset",
      "condition": "!!(@local.tips_data.image_1_asset)",
      "style_list": "flex: 1"
    },
    {
      "type": "image",
      "name": "image_2_asset",
      "value": "@local.tips_data.image_2_asset",
      "condition": "!!(@local.tips_data.image_2_asset)",
      "style_list": "flex: 1"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "text",
      "name": "intro_text",
      "value": "@local.tips_data.intro_text",
      "condition": "!!(@local.tips_data.intro_text)"
    },
    {
      "name": "media_title",
      "value": "@local.tips_data.media_title",
      "condition": "!!(@local.tips_data.media_title)"
    },
    {
      "type": "begin_display_group",
      "name": "video",
      "parameter_list": "style: column",
      "condition": "@local.tips_data.media_type == \"video\""
    },
    {
      "type": "title",
      "name": "print_media_title",
      "value": "@local.media_title",
      "condition": "!!(@local.tips_data.media_title)"
    },
    {
      "type": "video",
      "name": "media_asset ",
      "value": "@local.tips_data.media_asset "
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "begin_display_group",
      "name": "audio",
      "condition": "@local.tips_data.media_type == \"audio\""
    },
    {
      "type": "audio",
      "name": "media_asset ",
      "value": "@local.tips_data.media_asset ",
      "parameter_list": "title: @local.media_title",
      "condition": "!!(@local.tips_data.media_title)"
    },
    {
      "type": "audio",
      "name": "media_asset ",
      "value": "@local.tips_data.media_asset ",
      "condition": "!(@local.tips_data.media_title)"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "begin_items",
      "value": "@data.number"
    },
    {
      "name": "text_temp",
      "value": "text_@item.id"
    },
    {
      "name": "text",
      "value": "@local.data_list_name.@local.id.@local.text_temp"
    },
    {
      "name": "subtitle_temp",
      "value": "title_@item.id"
    },
    {
      "name": "subtitle",
      "value": "@local.data_list_name.@local.id.@local.subtitle_temp"
    },
    {
      "name": "image_temp",
      "value": "image_text_@item.id"
    },
    {
      "name": "image",
      "value": "@local.data_list_name.@local.id.@local.image_temp"
    },
    {
      "type": "begin_display_group",
      "name": "dg",
      "condition": "!!(@data.@local.text)"
    },
    {
      "type": "begin_display_group",
      "name": "dg_tips",
      "parameter_list": "variant: box_secondary;\nstyle: column;",
      "condition": "@item.id =<6"
    },
    {
      "type": "title",
      "name": "title_@item.id",
      "value": "@data.@local.subtitle"
    },
    {
      "type": "text",
      "name": "text_@item.id",
      "value": "@data.@local.text",
      "condition": "!!@data.@local.text",
      "style_list": "flex:3"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "end_display_group"
    },
    {
      "type": "end_items"
    },
    {
      "type": "text",
      "name": "outro_text",
      "value": "@local.tips_data.outro_text",
      "condition": "!!(@local.tips_data.outro_text)"
    },
    {
      "type": "begin_template",
      "name": "nav_buttons",
      "value": "nav_buttons",
      "condition": "@fields.show_nav_buttons",
      "action_list": "completed | emit: completed; uncompleted | emit: uncompleted;"
    },
    {
      "type": "end_template"
    }
  ]
}