{
  "flow_type": "data_pipe",
  "flow_name": "session_data_pipe",
  "status": "released",
  "rows": [
    {
      "operation": "map",
      "args_list": "session_id: @row.id;\nid: {@row.id}_tasks;\nnumber: @row.number;",
      "input_source": "session"
    },
    {
      "operation": "append_columns",
      "args_list": "completed_field: task_{@row.session_id}_completed;\nskipped_field: task_{@row.session_id}_skipped",
      "output_target": "session_tasks",
      "comments": "Append a column with the completed field"
    }
  ]
}